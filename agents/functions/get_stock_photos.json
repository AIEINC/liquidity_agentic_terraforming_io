                "content_filter": "low"
            },
            "metadata": {
                "designer": {
                    "x": 359,
                    "y": -3
                },
                "restore": {
                    "expect": {
                        "color": {
                            "mode": "chose",
                            "label": "Empty"
                        },
                        "order_by": {
                            "mode": "chose",
                            "label": "Relevant"
                        },
                        "orientation": {
                            "mode": "chose",
                            "label": "Landscape"
                        },
                        "content_filter": {
                            "mode": "chose",
                            "label": "Low"
                        }
                    },
                    "parameters": {
                        "__IMTCONN__": {
                            "data": {
                                "scoped": "true",
                                "connection": "unsplash"
